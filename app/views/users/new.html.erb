<%= form_for :user, url: users_path do |f| %>
  <p>
    <%= f.label :petal_id %><br>
    <%= f.text_field :petal_id %>
  </p>
  
  <p>
    <%= f.label :access_token %><br>
    <%= f.text_field :access_token %>
  </p>
  
  <p>
    <%= f.label :provider %><br>
    <%= f.text_field :provider %>
  </p>
  
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>
  
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  
  <p>
    <%= f.label :password %><br>
    <%= f.text_field :pass %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>